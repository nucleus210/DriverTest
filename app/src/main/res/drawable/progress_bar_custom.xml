<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:id="@android:id/background"
            android:shape="rectangle"
            android:thicknessRatio="89"
            android:useLevel="false">
            <corners
                android:bottomLeftRadius="21dp"
                android:bottomRightRadius="21dp" />
            <solid android:color="#34FAFAFA" />
            <size
                android:width="340dp"
                android:height="340dp" />
            <stroke
                android:width="1dp"
                android:color="#9a9a9a" />
        </shape>
    </item>

    <item>
        <clip>
            <shape
                android:id="@android:id/progress"
                android:shape="rectangle"
                android:thicknessRatio="89"
                android:useLevel="true">
                <corners
                    android:bottomLeftRadius="21dp"
                    android:bottomRightRadius="21dp" />
                <gradient
                    android:angle="270"
                    android:centerY="1.0"
                    android:endColor="#d2efef70"
                    android:startColor="#d24a4a23" />
                <size
                    android:width="340dp"
                    android:height="340dp" />
                <stroke
                    android:width="1dp"
                    android:color="#9a9a9a" />
            </shape>
        </clip>
    </item>
</layer-list>